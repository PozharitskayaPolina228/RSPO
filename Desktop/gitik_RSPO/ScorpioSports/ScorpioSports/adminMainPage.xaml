<Page x:Class="ScorpioSports.adminMainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ScorpioSports"
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="1280"
      Background="#eeeeee"
      Title="adminMainPage">
    <Grid>
        <Menu>
            <MenuItem Header="Профиль" FontSize="25" x:Name="profile" Click="profile_Click"/>
            <MenuItem Header="Пользователи" FontSize="25" x:Name="clients" Click="clients_Click"/>
            <MenuItem Header="Сотрудники" FontSize="25" x:Name="couch" Click="couch_Click"/>
            <MenuItem Header="Обновить" x:Name="update" Click="update_Click" FontSize="25"/>
        </Menu>
        <Frame x:Name="mainFrame" NavigationUIVisibility="Hidden" Margin="0,50,0,0"/>
    </Grid>
</Page>
